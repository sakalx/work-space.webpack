{
  "name": "webpack-work-space",
  "version": "1.0.0",
  "description": "front-end development",
  "main": "index.js",
  "browserslist": [
    "> 1%",
    "ie > 9"
  ],
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "ignore": "_*.*"
  },
  "scripts": {
    "svg-build": "npm run svg:clean && npm run svg:minify && npm run svg:sprites",
    "svg:clean": "gulp svg-clean",
    "svg:minify": "gulp svg-min",
    "svg:sprites": "gulp svg-sprite",
    "dev": "webpack-dev-server",
    "dev:ownServer": "webpack -d --watch",
    "prod": "cross-env NODE_ENV=production webpack",
    "stats": "webpack --profile --json > stats.json"
  },
  "author": "SakalS",
  "license": "ISC",
  "peerDependencies": {
    "webpack": "^2.0.0 || ^3.0.0"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "jquery": "^3.2.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.3",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^0.11.2",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-svg-sprite": "^1.3.7",
    "gulp-svgmin": "^1.2.4",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.0",
    "jquery": "^3.2.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  }
}
